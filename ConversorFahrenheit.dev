#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	float tempc, tempf ;
		printf ("Informe a temperatura em Graus Celsius: "); //solicita o valor da variável temp
		scanf("%f",& tempc); //lê o valor da variável temp
		tempf = (9 * tempc + 160) / 5; //efetua o cálculo de conversão	
	printf ("O valor da temperatura em Fahrenheit equivale a: %.2f",tempf); 		
	return 0;
}
